//accepted

#include<stdio.h>
int main()
{
	int a,b,c,T,P=1;
	//printf("Enter the numbr of test cases:\n");
	scanf("%d",&T);
		while( (T--) && (T<20))
		{
			//printf("Enter the salaries of 3 employee:\n");
			scanf("%d %d %d",&a,&b,&c);

			if(( a>b || a>c ) && ( a<c || a<b ))
			{
				printf("Case %d: %d\n",P++,a);
			}
			else if(( b>a || b>c ) && (b<c || b<a))
			{
				printf("Case %d: %d\n",P++,b);
			}
			else if((c>a || c>b) && ( c<b || c<a ))
			{
				printf("Case %d: %d\n",P++,c);
			}

		}
	
 
}
