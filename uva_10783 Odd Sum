#include<stdio.h>
int main()
{
	int p=1,c=0,a,b,sum=0,t;
	int Case[10];
	//printf("Enter the number of test cases:");
	scanf("%d\n",&t);
	while(p<=t)
	 {
    //printf("\nEnter the range\t");
	scanf("%d\n%d",&a,&b);
	//printf("Your range start from %d and Ends at %d\n",a,b);
			for(c=a;c<=b;c++)
			 {
				 if(c%2 !=0)
				 {
					 sum=sum+c;
				 }
			 } 
			printf("Case %d: ",p);
			printf("%d\n",sum);
	     	sum=0;
			p++;
	 } 
}
