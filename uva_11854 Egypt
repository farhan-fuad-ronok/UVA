#include<stdio.h>
int main()
{
	int s,a,b,c,d,p=0;
	while(p!=1)
	{
        scanf("%d %d %d",&a,&b,&c);
		if((a && b && c)<30000)
		{
		  if(a==0 && b==0 && c==0)
		   {
			 p=1;
			 break;
		   }
		  if((a*a)+(b*b)==(c*c)){printf("right\n");}
		  else if((a*a)+(c*c)==(b*b)){printf("right\n");}
		  else if((c*c)+(b*b)==(a*a)){printf("right\n");}
		  else {printf("wrong\n");}
		}	
	}
}
 
